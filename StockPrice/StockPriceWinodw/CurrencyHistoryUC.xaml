<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StockPriceWinodw"
             xmlns:syncfusion="http://schemas.syncfusion.com/wpf" x:Class="StockPriceWinodw.CurrencyHistoryUC"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <syncfusion:SfChart Header="Chart Header" HorizontalAlignment="Center" Margin="10" VerticalAlignment="Center">
            <syncfusion:SfChart.PrimaryAxis>
                <syncfusion:NumericalAxis FontSize="10" Header="Primary Axis" Maximum="10" Minimum="0" ShowGridLines="False"/>
            </syncfusion:SfChart.PrimaryAxis>
            <syncfusion:SfChart.SecondaryAxis>
                <syncfusion:NumericalAxis FontSize="10" Header="Secondary Axis" Maximum="100" Minimum="0"/>
            </syncfusion:SfChart.SecondaryAxis>
        </syncfusion:SfChart>
    </Grid>
</UserControl>
