    <!-- *********************************  Tooltip  ********************************* -->
    <Style x:Key="ToolTipStyle" TargetType="ContentControl">
        <Setter Property="Background" Value="{StaticResource LegendBgBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource LegendStroke}"/>
        <Setter Property="Foreground" Value="{StaticResource ForegroundBrush}"/>
        <Setter Property="FontSize" Value="14.667"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="Padding" Value="5,7"/>
